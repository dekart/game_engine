<% content_tag :div, :class => payout.name do %>
  <%= raw "%s (%s %s %d%%)" % [
        payout.property_type ? payout.property_type.name : content_tag(:span, t("payouts.property.missing"), :class => :missing),
        payout.apply_on,
        payout.action,
        payout.chance
      ] %>
<% end %>