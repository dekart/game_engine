<table id="mission_group_fields">
  <tr>
    <td>
      <%= form.text_field :name, :required => true %>

      <%= form.text_field :level, :size => 3, :required => true %>

      <%= form.text_field :title %>
    </td>
    <td>
      <div class="serializable" id="payout_list">
        <h2><%= MissionGroup.human_attribute_name("payouts") %></h2>

        <%= render :partial => "admin/payouts/collection", :locals => {:form => form} %>
      </div>
    </td>
  </tr>
</table>