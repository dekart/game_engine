<style type="text/css">
  h1.title {font-size: 18px; color: #3B5998; margin: 15px 0px;}
  h2,h3 {font-size: 16px; color: #3B5998;}
  td {vertical-align: top;}

  #header {height: 100px; border-bottom: 1px solid #d0d0d0;}

  #main_menu {display:block; clear:both; height: 25px; border-top: 1px solid #d0d0d0;}
    #main_menu a {float:left; display: block; height: 25px; line-height: 25px; font-weight:bold; padding: 0 10px 0 30px; background-position: 8px center; background-repeat: no-repeat; border-left: 1px solid white; border-right: 1px solid white  }
    #main_menu a:hover {border-color: #d0d0d0;}
    <% %w{home settings profile missions fights inventory shop}.each do |attr| %>
      #main_menu a.<%= attr %> {background-image: url(<%= image_path("icons/#{attr}.gif") %>)}
    <% end %>
    
  #co {width: 460px; float: right; padding:0;}
    #co li {float:left; width: 150px; display: block; height: 25px; margin: 0px; line-height: 25px; }
    #co b {display:block; float:left;}
    #co .first {width: 125px;}
      #co .first b {width: 50px;}
    #co .second {width: 155px;}
      #co .second b {width: 70px;}
    #co .third {width: 180px;}
      #co .third b {width: 50px;}
      #co .third div {width: 52px;}
    #co div {float: left;height: 20px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
    <% %w{basic_money vip_money level experience health energy}.each do |attr| %>
      #co_<%= attr %> {background-image: url(<%= image_path("icons/#{attr}.gif") %>);}
    <% end %>
    #co_points {padding-left: 3px !important;}
    #co .timer {color: #b0b0b0;padding-left: 5px;}

  #next_steps {margin: 10px 5px; border: 1px solid #EC7000; padding: 8px; background-color: #FFFFD5;}
    #next_steps h2 {color: #EC7000; margin-bottom: 15px;}
    #next_steps .step {font-size: 14px; font-weight: bold; margin-bottom: 15px; padding-left: 25px; background: transparent url(<%= image_path("icons/step.gif") %>) no-repeat top left; }
    #next_steps .fbml_add_profile_button {margin-top: 10px;}
    #next_steps .hide {float: right; padding-left: 14px; background: transparent url(<%= image_path("icons/hide.gif") %>) no-repeat center left;}

  .empty_set {margin: 10px 5px; padding: 8px; border: 1px solid #D8DFEA;}

  .pagination {margin: 10px 0px; text-align: right;}
    .pagination .disabled {display:none;}
    .pagination a, .pagination span {border: 1px solid #3b5998; padding: 3px;}
    .pagination a:hover {background-color: #d8dfea;}
    .pagination .current {background-color: #d8dfea; font-weight:bold; color: #3b5998;}

  form .required {color: red; padding-left: 5px;}
  form .field {margin-bottom: 5px;}
    form .field .label {margin-bottom: 3px;}
    form .field label {color: #3b5998;}
  form .fieldWithErrors {display:inline;}
    form .fieldWithErrors select{border: 1px solid #ee5555;}
    form .fieldWithErrors input{border: 1px solid #ee5555;}
    form .fieldWithErrors textarea{border: 1px solid #ee5555;}
  form .error {color: red; padding: 3px 0px;}

  .character_attributes {padding: 0; }
    .character_attributes li {display: block; clear:both;height: 25px; line-height: 25px;}
    .character_attributes li b {width: 60px;display:block; float: left}
    .character_attributes li .value {float:left;padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
    <% %w{attack defence health energy basic_money vip_money}.each do |attr| %>
      .character_attributes .<%= attr %> .value {background-image: url(<%= image_path("icons/#{attr}.gif") %>);}
    <% end %>

  #dashboard {margin-top: 15px;}
  #dashboard .profile {width: 250px;}
    #dashboard .profile .title {font-size: 15px; font-weight: bold;}
  #dashboard .fights td {padding: 10px 0;border-bottom: 1px solid #d0d0d0;}
    #dashboard .fights .fight {clear: both; margin: 5px 0; }
    #dashboard .fights .attacker {width: 55px;}
      #dashboard .fights .attacker img {border: 1px solid #d0d0d0;}
    #dashboard .fights .victim {width: 55px;}
      #dashboard .fights .victim img {border: 1px solid #d0d0d0;}
    #dashboard .fights .time {font-weight: bold; margin-bottom: 5px;}

  #mission_list td {padding: 10px 0px; border-bottom: 1px solid #d0d0d0;}
    #mission_list .details {padding-right: 10px;}
    #mission_list .details .name {font-size: 15px; margin-bottom: 5px;font-weight: bold;}
    #mission_list .payout {width: 100px;}
    <% %w{basic_money experience}.each do |attr| %>
      #mission_list .payout .<%= attr %> {height: 25px;line-height: 25px;padding-left: 20px; background: url(<%= image_path("icons/#{attr}.gif") %>) center left no-repeat;}
    <% end %>
    #mission_list .requires {width: 100px;}
      #mission_list .requires .energy {height: 25px;line-height: 25px;padding-left: 20px; background: url(<%= image_path("icons/energy.gif") %>) center left no-repeat;}
    #mission_list .controls a {display: block; padding: 5px 5px 5px 25px; margin: 5px; border: 1px solid #3B5998; font-weight:bold; background: url(<%= image_path("icons/missions.gif") %>) 3px center no-repeat;}
    #mission_list .controls a:hover {background-color: #e0e0e0;}

  .result {font-size: 13px;margin-bottom:10px;border: 2px solid #3B5998;}
    .result td {padding-right: 40px;}
    .result h3 {font-size: 13px;}
    .result p {font-size: 13px;}
    .result ul{padding:0;}
    .result li {display: block; height: 22px;line-height: 22px;}
    .result li b {display: block; width: 75px; float:left;}
    .result li div {float:left;height: 22px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
    <% %w{experience basic_money energy health}.each do |attr| %>
      .result .<%= attr %> div{background-image: url(<%= image_path("icons/#{attr}.gif") %>)}
    <% end %>
    
  #mission_result {padding:0px 30px 20px 30px; background: url(<%= image_path("icons/missions.gif") %>) 5px 8px no-repeat;}
    #mission_result .spent b {width: 55px;}
    #mission_result .title {line-height: 22px; padding-left: 23px; background: url(<%= image_path("icons/level.gif") %>) center left no-repeat;}
    #mission_result .fulfill_again {display:block; padding-left: 25px;width: 90px; height: 25px; line-height: 25px; margin-bottom: 10px; border: 1px solid #3B5998; font-weight: bold;background: url(<%= image_path("icons/missions.gif") %>) 3px center no-repeat;}
    #mission_result .fulfill_again:hover {background-color: #e0e0e0;}

  #victim_list td{padding: 10px 0; border-bottom: 1px solid #d0d0d0;vertical-align:middle;}
    #victim_list .name{ width: 200px;}
    #victim_list .level {width: 50px;}
    #victim_list .controls a {display: block; padding: 5px 5px 5px 25px; margin: 5px; border: 1px solid #3B5998; font-weight:bold; background: url(<%= image_path("icons/fights.gif") %>) 3px center no-repeat;}
    #victim_list .controls a:hover {background-color: #e0e0e0;}

  #fight_result {padding:0px 30px; background: url(<%= image_path("icons/fights.gif") %>) 5px 8px no-repeat;}
    #fight_result .lost b {width: 55px;}
    #fight_result .victim_lost b {width: 55px;}
    #fight_result .attack_again {display:block; padding-left: 25px;width: 90px; height: 25px; line-height: 25px; margin-bottom: 10px; border: 1px solid #3B5998; font-weight: bold;background: url(<%= image_path("icons/fights.gif") %>) 3px center no-repeat;}
    #fight_result .attack_again:hover {background-color: #e0e0e0;}
    #fight_result .success {color:green;}
    #fight_result .fail {color: red;}

  #item_list td{padding: 10px 0; border-bottom: 1px solid #d0d0d0;}
    #item_list .info {padding-right: 10px;}
    #item_list .info .name {font-size: 15px; margin-bottom: 5px;font-weight: bold;}
    #item_list .details {width: 70px;}
      #item_list .details div {font-weight:bold;line-height: 22px;height: 22px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
      <% %w{attack defence}.each do |attr| %>
        #item_list .details .<%= attr %>{background-image: url(<%= image_path("icons/#{attr}.gif") %>)}
      <% end %>
    #item_list .requires {width: 50px;}
      #item_list .requires div {line-height: 22px;height: 22px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
      #item_list .requires .basic_price{background-image: url(<%= image_path("icons/basic_money.gif") %>)}
      #item_list .requires .vip_price{background-image: url(<%= image_path("icons/vip_money.gif") %>)}
    #item_list .spacer {border:0;}
    #item_list .controls a {display: block; padding: 5px 5px 5px 25px; margin: 5px; border: 1px solid #3B5998; font-weight:bold; background: url(<%= image_path("icons/basic_money.gif") %>) 3px center no-repeat;}
    #item_list .controls a:hover {background-color: #e0e0e0;}

  #shop_result {padding:0px 30px; background: url(<%= image_path("icons/basic_money.gif") %>) 5px 8px no-repeat;}

  #inventory_list td{padding: 10px 0; border-bottom: 1px solid #d0d0d0;}
    #inventory_list .info {padding-right: 10px;}
    #inventory_list .info .name {font-size: 15px; margin-bottom: 5px;font-weight: bold;}
    #inventory_list .details {width: 70px;}
      #inventory_list .details div {font-weight:bold;line-height: 22px;height: 22px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
      <% %w{attack defence}.each do |attr| %>
        #inventory_list .details .<%= attr %>{background-image: url(<%= image_path("icons/#{attr}.gif") %>)}
      <% end %>
    #inventory_list .spacer {border:0;}
    #inventory_list .sell {width: 70px;}
      #inventory_list .sell div {line-height: 22px;height: 22px; padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
      #inventory_list .sell .basic_money{background-image: url(<%= image_path("icons/basic_money.gif") %>)}
    #inventory_list .controls a {display: block; padding: 5px 5px 5px 25px; margin: 5px; border: 1px solid #3B5998; font-weight:bold;background-position: 3px center; background-repeat: no-repeat;}
    #inventory_list .controls a:hover {background-color: #e0e0e0;}
      #inventory_list .controls .sell {background-image: url(<%= image_path("icons/basic_money.gif") %>);}
      #inventory_list .controls .apply {background-image: url(<%= image_path("icons/add.gif") %>);}

  #inventory_result {padding:0px 30px; background: url(<%= image_path("icons/basic_money.gif") %>) 5px 8px no-repeat;}

  #upgrade_list td{padding: 10px; height: 25px; line-height: 25px;}
  #upgrade_list .points td{border-bottom: 1px solid #d0d0d0;}
  #upgrade_list .name {font-weight: bold;}
  #upgrade_list .value {padding-left: 20px; background-position: center left; background-repeat: no-repeat;}
  <% %w{attack defence health energy}.each do |attr| %>
    #upgrade_<%= attr %> {background-image: url(<%= image_path("icons/#{attr}.gif") %>);}
  <% end %>

  #upgrade_result {padding:0px 30px; background: url(<%= image_path("icons/add.gif") %>) 5px 8px no-repeat;}
</style>