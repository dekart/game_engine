<% fb_js_string :payout_fields do %>
  <%= render :partial => "admin/missions/payouts/#{@payout.name}", :locals => {:payout => @payout} %>
<% end %>

<% content_for :script do %>
  var new_payout = document.createElement("div");

  new_payout.
    setId('payout_<%= @payout.object_id %>').
    setClassName("payout").
    setInnerFBML(payout_fields);

  $('payout_list').appendChild(new_payout)
<% end %>