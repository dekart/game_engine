<%= render_to_result :market do |builder| %>
  <% builder.success do %>
    <%= t(".message").html_safe %>
  <% end %>

  <% builder.on_ready do %>
    $('#<%= dom_id(@market_item) %>').remove();

    <%= ga_track_event('Items', 'Market Listing Removed', "#{@market_item.name} (#{@market_item.inventory.item.availability})") %>
  <% end %>
<% end %>
