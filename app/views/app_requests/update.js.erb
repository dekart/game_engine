<% if @app_request.accepted? %>
  <%= render("app_requests/update/#{@app_request.type_name}", :app_request => @app_request) %>
  
  $('#<%= dom_id(@app_request) %> .ignore').empty();
<% else %>
  $('#<%= dom_id(@app_request) %> .controls').html('<%= escape_javascript(@app_request.errors[:base].join("\n")) %>');
<% end %>